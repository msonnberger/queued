		"svelte": "3.58.0",
		"svelte-check": "3.3.0",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.0",
		"typescript": "5.0.4",
		"vite": "4.3.4",
		"vitest": "0.31.0"
	},
	"packageManager": "pnpm@8.4.0",
	"engines": {
		"pnpm": "^8.0.0"
	},
	"type": "module",
	"dependencies": {
