{
	"name": "queued",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test:browser": "playwright test",
		"test:unit": "vitest run",
		"test": "pnpm test:unit && pnpm test:browser",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:migrate": "drizzle-kit generate:pg --schema=./src/lib/server/db/schema.ts --out=./src/lib/server/db/migrations/"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "4.0.2",
		"@playwright/test": "1.35.1",
		"@sveltejs/adapter-vercel": "3.0.1",
		"@sveltejs/kit": "1.22.1",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "4.0.3",
		"@types/pg": "8.10.2",
		"@types/qrcode": "1.5.1",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"autoprefixer": "10.4.14",
		"dotenv": "16.3.1",
		"drizzle-kit": "0.19.3",
		"eslint": "8.44.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"jsdom": "22.1.0",
		"openapi-types": "12.1.3",
		"postcss": "8.4.25",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"prettier-plugin-tailwindcss": "0.3.0",
		"svelte": "4.0.5",
		"svelte-check": "3.4.5",
		"tailwindcss": "3.3.2",
		"tslib": "2.6.0",
		"typescript": "5.1.6",
		"vite": "4.4.2",
		"vitest": "0.33.0"
	},
	"packageManager": "pnpm@8.6.7",
	"engines": {
		"pnpm": "^8.0.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-postgresql": "1.0.1",
		"@lucia-auth/oauth": "1.2.1",
		"@vercel/analytics": "1.0.1",
		"class-variance-authority": "0.6.1",
		"drizzle-orm": "0.27.1",
		"lucia-auth": "1.8.0",
		"lucide-svelte": "0.259.0",
		"oazapfts": "4.7.1",
		"pg": "8.11.1",
		"pusher-http-edge": "0.4.0",
		"pusher-js": "8.2.0",
		"qrcode": "1.5.3",
		"svelte-french-toast": "1.1.0",
		"svelte-headlessui": "0.0.18",
		"svelte-transition": "0.0.10",
		"tailwind-merge": "1.13.2",
		"zod": "3.21.4"
	}
}
