{
	"name": "queued",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test:browser": "playwright test",
		"test:unit": "vitest run",
		"test": "pnpm test:unit && pnpm test:browser",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:migrate": "drizzle-kit generate:pg --schema=./src/lib/server/db/schema.ts --out=./src/lib/server/db/migrations/"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "3.7.2",
		"@playwright/test": "1.34.3",
		"@sveltejs/adapter-vercel": "3.0.0",
		"@sveltejs/kit": "1.20.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.2",
		"@types/pg": "8.10.1",
		"@types/qrcode": "1.5.0",
		"@typescript-eslint/eslint-plugin": "5.59.7",
		"@typescript-eslint/parser": "5.59.7",
		"autoprefixer": "10.4.14",
		"dotenv": "16.0.3",
		"drizzle-kit": "0.18.1",
		"eslint": "8.41.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"jsdom": "22.1.0",
		"openapi-types": "12.1.3",
		"postcss": "8.4.24",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"prettier-plugin-tailwindcss": "0.3.0",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.2",
		"typescript": "5.0.4",
		"vite": "4.3.9",
		"vitest": "0.31.1"
	},
	"packageManager": "pnpm@8.5.1",
	"engines": {
		"pnpm": "^8.0.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-postgresql": "1.0.1",
		"@lucia-auth/oauth": "1.1.1",
		"@vercel/analytics": "1.0.1",
		"class-variance-authority": "0.6.0",
		"drizzle-orm": "0.26.1",
		"lucia-auth": "1.7.1",
		"lucide-svelte": "0.224.0",
		"oazapfts": "4.7.1",
		"pg": "8.11.0",
		"pusher-http-edge": "0.4.0",
		"pusher-js": "8.0.2",
		"qrcode": "1.5.3",
		"svelte-french-toast": "1.0.3",
		"svelte-headlessui": "0.0.15",
		"svelte-transition": "0.0.7",
		"tailwind-merge": "1.12.0",
		"zod": "3.21.4"
	}
}
