{
	"name": "queued",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test:browser": "playwright test",
		"test:unit": "vitest run",
		"test": "pnpm test:unit && pnpm test:browser",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:migrate": "drizzle-kit generate:pg --schema=./src/lib/server/db/schema.ts --out=./src/lib/server/db/migrations/"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "4.1.1",
		"@playwright/test": "1.41.2",
		"@sveltejs/adapter-vercel": "3.1.0",
		"@sveltejs/kit": "1.30.3",
		"@testing-library/jest-dom": "5.17.0",
		"@testing-library/svelte": "4.1.0",
		"@types/pg": "8.11.0",
		"@types/qrcode": "1.5.5",
		"@typescript-eslint/eslint-plugin": "6.21.0",
		"@typescript-eslint/parser": "6.21.0",
		"autoprefixer": "10.4.17",
		"dotenv": "16.4.4",
		"drizzle-kit": "0.20.14",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte3": "4.0.0",
		"jsdom": "22.1.0",
		"openapi-types": "12.1.3",
		"postcss": "8.4.35",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.0",
		"prettier-plugin-tailwindcss": "0.5.11",
		"svelte": "4.2.10",
		"svelte-check": "3.6.4",
		"tailwindcss": "3.4.1",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "4.5.2",
		"vitest": "0.34.6"
	},
	"packageManager": "pnpm@8.15.2",
	"engines": {
		"pnpm": "^8.0.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-postgresql": "1.0.1",
		"@lucia-auth/oauth": "1.2.1",
		"@vercel/analytics": "1.1.3",
		"class-variance-authority": "0.7.0",
		"drizzle-orm": "0.29.3",
		"lucia-auth": "1.8.0",
		"lucide-svelte": "0.330.0",
		"oazapfts": "4.12.0",
		"pg": "8.11.3",
		"pusher-http-edge": "0.4.0",
		"pusher-js": "8.3.0",
		"qrcode": "1.5.3",
		"svelte-french-toast": "1.2.0",
		"svelte-headlessui": "0.0.33",
		"svelte-transition": "0.0.10",
		"tailwind-merge": "1.14.0",
		"zod": "3.22.4"
	}
}
