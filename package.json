{
	"name": "queued",
	"version": "0.12.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test:browser": "playwright test",
		"test:unit": "vitest run",
		"test": "pnpm test:unit && pnpm test:browser",
		"gen:types": "pnpm dlx supabase gen types typescript --linked > ./src/lib/types/supabase.ts",
