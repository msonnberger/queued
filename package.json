{
	"name": "queued",
	"version": "0.2.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test:browser": "playwright test",
		"test:unit": "vitest run",
		"test": "pnpm test:unit && pnpm test:browser",
		"gen:types": "pnpm dlx supabase gen types typescript --linked > ./src/lib/types/supabase.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@playwright/test": "1.31.2",
		"@sveltejs/adapter-vercel": "2.3.1",
		"@sveltejs/kit": "1.11.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.2",
		"@types/qrcode": "1.5.0",
		"@typescript-eslint/eslint-plugin": "5.54.1",
		"@typescript-eslint/parser": "5.54.1",
		"autoprefixer": "10.4.14",
		"dotenv": "16.0.3",
		"eslint": "8.35.0",
		"eslint-config-prettier": "8.7.0",
		"eslint-plugin-svelte3": "4.0.0",
		"jsdom": "21.1.0",
		"openapi-types": "12.1.0",
		"postcss": "8.4.21",
		"prettier": "2.8.4",
		"prettier-plugin-svelte": "2.9.0",
		"prettier-plugin-tailwindcss": "0.2.4",
		"svelte": "3.56.0",
		"svelte-check": "3.1.0",
		"tailwindcss": "3.2.7",
		"tslib": "2.5.0",
		"typescript": "4.9.5",
		"vite": "4.1.4",
		"vitest": "0.29.2"
	},
	"packageManager": "pnpm@7.29.1",
	"engines": {
		"pnpm": "^7.0.0"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "0.9.0",
		"@supabase/supabase-js": "2.10.0",
		"class-variance-authority": "0.4.0",
		"lucide-svelte": "0.124.0",
		"oazapfts": "4.5.2",
		"pusher-http-edge": "0.3.0",
		"pusher-js": "8.0.1",
		"qrcode": "1.5.1",
		"svelte-french-toast": "1.0.3",
		"svelte-headlessui": "0.0.15",
		"svelte-transition": "0.0.7",
		"tailwind-merge": "1.10.0",
		"zod": "3.21.4"
	}
}
